{
  "name": "tree-sitter-datastar",
  "version": "0.1.0",
  "description": "Tree-sitter grammar for Datastar - hypermedia framework with reactive attributes",
  "repository": {
    "type": "git",
    "url": "https://github.com/YuryKL/tree-sitter-datastar.git"
  },
  "homepage": "https://github.com/YuryKL/tree-sitter-datastar#readme",
  "bugs": {
    "url": "https://github.com/YuryKL/tree-sitter-datastar/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Yury Kleyman",
    "email": "kleymanyy@gmail.com",
    "url": "https://github.com/YuryKL"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "tree-sitter",
    "parser",
    "datastar",
    "hypermedia",
    "syntax-highlighting",
    "neovim",
    "helix",
    "html",
    "javascript",
    "reactive",
    "incremental-parsing"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.2.1",
    "node-gyp-build": "^4.8.2",
    "tree-sitter-javascript": "^0.21.0"
  },
  "devDependencies": {
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "^0.25.3"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.1"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js",
    "prebuildify": "prebuildify --napi --strip"
  },
  "tree-sitter": [
    {
      "scope": "source.datastar",
      "injection-regex": "^datastar$"
    }
  ]
}
