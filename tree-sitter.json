{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "datastar",
      "camelcase": "Datastar",
      "title": "Datastar",
      "scope": "source.datastar",
      "file-types": [
        "html"
      ],
      "injection-regex": "^datastar$",
      "class-name": "TreeSitterDatastar"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Grammar for Datastar @ data-star.dev",
    "authors": [
      {
        "name": "Yury Kleyman",
        "email": "kleymanyy@gmail.com",
        "url": "https://github.com/YuryKL"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-datastar"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": true
  }
}
